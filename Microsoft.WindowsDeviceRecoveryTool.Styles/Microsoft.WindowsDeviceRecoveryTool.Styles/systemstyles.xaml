			Property="UIElement.SnapsToDevicePixels"
			Value="True" />
		<windows:Setter
			Property="FrameworkElement.OverridesDefaultStyle"
			Value="True" />
		<windows:Setter
			Property="FrameworkElement.FocusVisualStyle"
			Value="{x:Null}" />
		<windows:Setter
			Property="Foreground"
			Value="{DynamicResource CheckBoxContentForegroundThemeBrush}" />
		<windows:Setter
			Property="FontFamily"
			Value="{DynamicResource ContentControlThemeFontFamily}" />
		<windows:Setter
			Property="FontSize"
			Value="{DynamicResource ControlContentThemeFontSize}" />
		<windows:Setter
			Property="Template">
			<windows:Setter.Value>
				<ControlTemplate
					TargetType="{x:Type ToggleButton}">
					<BulletDecorator
						Background="#00FFFFFF">
						<BulletDecorator.Bullet>
							<Grid
								Name="SwitchTrack"
								windows:Width="50">
								<windows:FrameworkElement.Resources>
									<windows:ResourceDictionary>
										<windows:Thickness
											x:Key="Zero">0,0,0,0</windows:Thickness>
										<windows:Thickness
											x:Key="NonZero">35,0,0,0</windows:Thickness>
										<converters:GenericBoolConverter
											x:Key="ThicknessConverter"
											FalseValue="{windows:StaticResource Zero}"
											TrueValue="{windows:StaticResource NonZero}" />
									</windows:ResourceDictionary>
								</windows:FrameworkElement.Resources>
								<Grid
									Name="SwitchBottom"
									Background="{windows:DynamicResource ToggleSwitchOuterBorderBorderThemeBrush}"
									windows:Height="20">
									<Rectangle
										Name="SwitchBackground"
										windows:Width="48"
										windows:Height="20"
										Fill="{windows:DynamicResource ToggleSwitchTrackBackgroundThemeBrush}" />
									<Border
										BorderBrush="{windows:DynamicResource ToggleSwitchOuterBorderBorderThemeBrush}"
										BorderThickness="2">
										<Border
											Name="InnerBox"
											BorderBrush="{windows:DynamicResource ApplicationPageBackgroundThemeBrush}"
											BorderThickness="2" />
									</Border>
								</Grid>
								<Border
									Name="SwitchThumb"
									BorderBrush="{windows:DynamicResource ToggleSwitchThumbBackgroundThemeBrush}"
									BorderThickness="1"
									windows:Width="15"
									windows:Height="22"
									windows:HorizontalAlignment="Left"
									windows:Margin="{Binding IsChecked, Mode=OneTime, RelativeSource={RelativeSource TemplatedParent}, Converter={windows:StaticResource ThicknessConverter}}">
									<Border
										Name="ThumbCenter"
										BorderBrush="{windows:DynamicResource ToggleSwitchThumbBorderThemeBrush}"
										BorderThickness="2"
										Background="{windows:DynamicResource ToggleSwitchThumbBackgroundThemeBrush}" />
								</Border>
								<Rectangle
									Name="FocusVisualWhite"
									windows:UseLayoutRounding="True"
									IsHitTestVisible="False"
									Stroke="{windows:DynamicResource FocusVisualWhiteStrokeThemeBrush}"
									StrokeEndLineCap="Square"
									StrokeDashArray="1,1"
									Opacity="0"
									StrokeDashOffset="1.5" />
								<Rectangle
									Name="FocusVisualBlack"
									windows:UseLayoutRounding="True"
									IsHitTestVisible="False"
									Stroke="{windows:DynamicResource FocusVisualBlackStrokeThemeBrush}"
									StrokeEndLineCap="Square"
									StrokeDashArray="1,1"
									Opacity="0"
									StrokeDashOffset="0.5" />
							</Grid>
						</BulletDecorator.Bullet>
						<windows:VisualStateManager.VisualStateGroups>
							<windows:VisualStateGroup
								x:Name="CommonStates">
								<windows:VisualState
									x:Name="Normal" />
								<windows:VisualState
									x:Name="MouseOver" />
								<windows:VisualState
									x:Name="Pressed">
									<Storyboard>
										<DoubleAnimation
											Storyboard.TargetName="FocusVisualWhite"
											Storyboard.TargetProperty="Opacity"
											To="0"
											Duration="0" />
										<DoubleAnimation
											Storyboard.TargetName="FocusVisualBlack"
											Storyboard.TargetProperty="Opacity"
											To="0"
											Duration="0" />
									</Storyboard>
								</windows:VisualState>
								<windows:VisualState
									x:Name="Disabled" />
							</windows:VisualStateGroup>
							<windows:VisualStateGroup
								x:Name="CheckStates">
								<windows:VisualState
									x:Name="Checked">
									<Storyboard>
										<ThicknessAnimation
											Storyboard.TargetProperty="Margin"
											Storyboard.TargetName="SwitchThumb"
											To="35,0,0,0"
											Duration="0:0:0.1" />
										<ObjectAnimationUsingKeyFrames
											Storyboard.TargetName="SwitchBackground"
											Storyboard.TargetProperty="Fill">
											<DiscreteObjectKeyFrame
												KeyTime="0"
												Value="{windows:DynamicResource ToggleSwitchCurtainBackgroundThemeBrush}" />
										</ObjectAnimationUsingKeyFrames>
									</Storyboard>
								</windows:VisualState>
								<windows:VisualState
									x:Name="Unchecked">
									<Storyboard>
										<ThicknessAnimation
											Storyboard.TargetProperty="Margin"
											Storyboard.TargetName="SwitchThumb"
											To="0,0,0,0"
											Duration="0:0:0.1" />
									</Storyboard>
								</windows:VisualState>
								<windows:VisualState
									x:Name="Indeterminate" />
							</windows:VisualStateGroup>
							<windows:VisualStateGroup
								x:Name="FocusStates">
								<windows:VisualState
									x:Name="Focused">
									<Storyboard>
										<DoubleAnimation
											Storyboard.TargetName="FocusVisualWhite"
											Storyboard.TargetProperty="Opacity"
											To="1"
											Duration="0" />
										<DoubleAnimation
											Storyboard.TargetName="FocusVisualBlack"
											Storyboard.TargetProperty="Opacity"
											To="1"
											Duration="0" />
									</Storyboard>
								</windows:VisualState>
								<windows:VisualState
									x:Name="Unfocused" />
