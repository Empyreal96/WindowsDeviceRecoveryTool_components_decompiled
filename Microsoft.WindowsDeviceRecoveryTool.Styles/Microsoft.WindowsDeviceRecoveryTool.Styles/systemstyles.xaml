			Property="Template">
			<windows:Setter.Value>
				<ControlTemplate
					TargetType="{x:Type ScrollViewer}">
					<ControlTemplate.Triggers>
						<windows:Trigger
							Property="UIElement.IsVisible"
							Value="True">
							<windows:TriggerBase.EnterActions>
								<BeginStoryboard>
									<Storyboard>
										<DoubleAnimation
											Storyboard.TargetName="PART_VerticalScrollBar"
											Storyboard.TargetProperty="Opacity"
											From="0"
											To="1"
											Duration="0:0:0.5" />
										<DoubleAnimation
											Storyboard.TargetName="PART_HorizontalScrollBar"
											Storyboard.TargetProperty="Opacity"
											From="0"
											To="1"
											Duration="0:0:0.5" />
									</Storyboard>
								</BeginStoryboard>
							</windows:TriggerBase.EnterActions>
						</windows:Trigger>
					</ControlTemplate.Triggers>
					<Grid>
						<Grid.ColumnDefinitions>
							<ColumnDefinition
								Width="*" />
							<ColumnDefinition
								Width="Auto" />
						</Grid.ColumnDefinitions>
						<Grid.RowDefinitions>
							<RowDefinition
								Height="*" />
							<RowDefinition
								Height="Auto" />
						</Grid.RowDefinitions>
						<ScrollContentPresenter
							Grid.ColumnSpan="2"
							Grid.RowSpan="2" />
