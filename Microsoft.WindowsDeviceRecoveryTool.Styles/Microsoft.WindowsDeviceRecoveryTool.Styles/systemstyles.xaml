						<windows:Trigger
							Property="UIElement.IsFocused"
							Value="True">
							<windows:Setter
								TargetName="FocusVisualWhite"
								Property="UIElement.Opacity"
								Value="1" />
							<windows:Setter
								TargetName="FocusVisualBlack"
								Property="UIElement.Opacity"
								Value="1" />
						</windows:Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</windows:Setter.Value>
