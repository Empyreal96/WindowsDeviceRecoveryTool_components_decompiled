			Property="Template">
			<windows:Setter.Value>
				<ControlTemplate
					TargetType="{x:Type ListBox}">
					<Border
						Background="{windows:TemplateBinding Background}"
						BorderBrush="{windows:TemplateBinding BorderBrush}"
						BorderThickness="{windows:TemplateBinding BorderThickness}">
						<ScrollViewer
							windows:Name="ScrollViewer"
							VerticalScrollBarVisibility="Disabled"
							HorizontalScrollBarVisibility="Auto"
							behaviors:ScrollBehavior.HorizontalScroll="True"
							behaviors:ScrollBehavior.MouseWheelScrollValue="{windows:TemplateBinding behaviors:MouseWheelScrollValue}"
							Focusable="False">
							<ItemsPresenter
								windows:Margin="{windows:TemplateBinding Padding}"
								Focusable="False"
								windows:HorizontalAlignment="Stretch"
								windows:VerticalAlignment="Stretch" />
