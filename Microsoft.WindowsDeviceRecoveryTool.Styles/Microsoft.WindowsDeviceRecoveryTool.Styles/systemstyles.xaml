								TargetName="Background"
								Value="{DynamicResource ComboBoxPointerOverBackgroundThemeBrush}"
								Property="Background" />
							<windows:Setter
								TargetName="Background"
								Value="{DynamicResource ComboBoxPointerOverBorderThemeBrush}"
								Property="BorderBrush" />
							<windows:Setter
								TargetName="Highlight"
								Value="{DynamicResource ComboBoxSelectedPointerOverBackgroundThemeBrush}"
								Property="Fill" />
						</windows:Trigger>
						<windows:Trigger
							Property="IsEnabled"
