	<windows:Style
		TargetType="{x:Type ToggleButton}"
		x:Key="ExpanderToggleButtonStyle">
		<windows:Setter
			Property="Template">
			<windows:Setter.Value>
				<ControlTemplate
					TargetType="{x:Type ToggleButton}">
					<Grid
						SnapsToDevicePixels="False"
						Background="#00FFFFFF">
						<Grid.ColumnDefinitions>
							<ColumnDefinition
								Width="Auto" />
							<ColumnDefinition
								Width="*" />
						</Grid.ColumnDefinitions>
						<Path
							Name="Arrow"
							windows:Width="6"
							windows:Height="10"
							Grid.Column="0"
							SnapsToDevicePixels="False"
							windows:VerticalAlignment="Center"
							windows:HorizontalAlignment="Center"
							Fill="#00FFFFFF"
							Stroke="{windows:TemplateBinding Foreground}"
							StrokeThickness="1"
							Stretch="Fill"
							Data="M0,0 L4,4 L0,8">
							<UIElement.RenderTransform>
								<RotateTransform
									CenterX="3"
									CenterY="5"
									Angle="0" />
							</UIElement.RenderTransform>
						</Path>
						<TextBlock
							Grid.Column="1"
							windows:Style="{windows:DynamicResource BasicTextStyle}"
							windows:Margin="5,0,5,0"
							Text="{Binding Path=Content, RelativeSource={RelativeSource TemplatedParent}}" />
					</Grid>
					<ControlTemplate.Triggers>
						<windows:Trigger
							Property="ToggleButton.IsChecked"
							Value="True">
							<windows:TriggerBase.EnterActions>
								<BeginStoryboard>
									<Storyboard>
										<DoubleAnimation
											Storyboard.TargetName="Arrow"
											Storyboard.TargetProperty="(Path.RenderTransform).(RotateTransform.Angle)"
											To="90"
											Duration="0:0:0.1" />
									</Storyboard>
								</BeginStoryboard>
							</windows:TriggerBase.EnterActions>
							<windows:TriggerBase.ExitActions>
								<BeginStoryboard>
									<Storyboard>
										<DoubleAnimation
											Storyboard.TargetName="Arrow"
											Storyboard.TargetProperty="(Path.RenderTransform).(RotateTransform.Angle)"
											To="0"
											Duration="0:0:0.1" />
									</Storyboard>
								</BeginStoryboard>
							</windows:TriggerBase.ExitActions>
						</windows:Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</windows:Setter.Value>
		</windows:Setter>
	</windows:Style>
	<ControlTemplate
		TargetType="{x:Type Expander}"
		x:Key="RevealExpander">
		<DockPanel>
			<ToggleButton
				Name="ExpanderButton"
				DockPanel.Dock="Top"
				Content="{windows:TemplateBinding Header}"
				windows:OverridesDefaultStyle="True"
				Padding="1.5,0"
				windows:Style="{windows:StaticResource ExpanderToggleButtonStyle}"
				IsChecked="{Binding Path=IsExpanded, RelativeSource={RelativeSource TemplatedParent}}" />
			<ContentPresenter
				Name="ExpanderContent"
				Visibility="Collapsed"
				ContentSource="Content" />
		</DockPanel>
		<ControlTemplate.Triggers>
			<windows:Trigger
				Property="Expander.IsExpanded"
				Value="True">
				<windows:Setter
					TargetName="ExpanderContent"
					Property="Visibility"
					Value="Visible" />
			</windows:Trigger>
		</ControlTemplate.Triggers>
