							windows:Name="PART_VerticalScrollBar"
							Opacity="0"
							Grid.Column="1"
							Template="{windows:DynamicResource VerticalScrollBar}"
							Background="{windows:DynamicResource ApplicationPageBackgroundThemeBrush}"
