				Value="True"
				Binding="{Binding IsDeleted}">
				<windows:TriggerBase.EnterActions>
					<BeginStoryboard>
						<Storyboard>
							<DoubleAnimation
								Storyboard.TargetProperty="ScaleY"
								Storyboard.TargetName="ScaleTransform"
								From="1"
								To="0.9"
								Duration="0:0:0.2" />
							<DoubleAnimation
								Storyboard.TargetProperty="ScaleX"
								Storyboard.TargetName="ScaleTransform"
								From="1"
								To="0.9"
								Duration="0:0:0.2" />
							<DoubleAnimation
								Storyboard.TargetProperty="Y"
								Storyboard.TargetName="TranslateTransform"
								To="50"
								Duration="0:0:0.15"
								BeginTime="0:0:0.2" />
							<DoubleAnimation
								Storyboard.TargetProperty="Opacity"
								Duration="00:00:0.15"
								To="0"
								BeginTime="0:0:0.2" />
						</Storyboard>
					</BeginStoryboard>
				</windows:TriggerBase.EnterActions>
			</windows:DataTrigger>
			<windows:DataTrigger
				Value="True"
				Binding="{Binding ShowStartAnimation}">
				<windows:TriggerBase.EnterActions>
					<BeginStoryboard>
						<Storyboard>
							<DoubleAnimation
								Storyboard.TargetProperty="ScaleY"
								Storyboard.TargetName="ScaleTransform"
								To="0.9"
								Duration="0:0:0" />
							<DoubleAnimation
								Storyboard.TargetProperty="ScaleX"
								Storyboard.TargetName="ScaleTransform"
								To="0.9"
								Duration="0:0:0" />
							<DoubleAnimation
								Storyboard.TargetProperty="Opacity"
								Duration="00:00:0.15"
								From="0"
								To="1" />
