								TargetName="HighlightBackground"
								Property="UIElement.Opacity"
								Value="1" />
							<windows:Setter
								TargetName="Highlight"
								Property="UIElement.Opacity"
								Value="1" />
							<windows:Setter
								TargetName="ContentPresenter"
								Value="{DynamicResource ComboBoxFocusedForegroundThemeBrush}"
								Property="Foreground" />
						</windows:Trigger>
						<windows:Trigger
							SourceName="Toggle"
							Property="IsPressed"
							Value="True">
							<windows:Setter
								TargetName="Background"
								Value="{DynamicResource ComboBoxPressedBackgroundThemeBrush}"
								Property="Background" />
							<windows:Setter
								TargetName="Background"
								Value="{DynamicResource ComboBoxPressedBorderThemeBrush}"
								Property="BorderBrush" />
							<windows:Setter
								TargetName="ContentPresenter"
								Value="{DynamicResource ComboBoxPressedForegroundThemeBrush}"
								Property="Foreground" />
							<windows:Setter
								TargetName="PressedBackground"
