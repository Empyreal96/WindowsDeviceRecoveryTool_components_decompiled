											To="1"
											Duration="0" />
										<DoubleAnimation
											Storyboard.TargetName="FocusVisualBlack"
											Storyboard.TargetProperty="Opacity"
											To="1"
											Duration="0" />
									</Storyboard>
								</windows:VisualState>
								<windows:VisualState
									x:Name="Unfocused" />
							</windows:VisualStateGroup>
						</windows:VisualStateManager.VisualStateGroups>
						<Grid>
							<Grid.ColumnDefinitions>
								<ColumnDefinition
									Width="27" />
								<ColumnDefinition
									Width="*" />
							</Grid.ColumnDefinitions>
							<Grid
								windows:VerticalAlignment="Top">
								<Rectangle
									Name="NormalRectangle"
									Fill="{windows:DynamicResource CheckBoxBackgroundThemeBrush}"
									Stroke="{windows:DynamicResource CheckBoxBorderThemeBrush}"
									StrokeThickness="{windows:DynamicResource CheckBoxBorderThemeThickness}"
									windows:UseLayoutRounding="False"
									windows:Height="21"
									windows:Width="21" />
								<Path
									Name="CheckGlyph"
									Data="F1 M0,58 L2,56 L6,60 L13,51 L15,53 L6,64"
									Fill="{windows:DynamicResource CheckBoxForegroundThemeBrush}"
									windows:Height="14"
									Stretch="Fill"
									windows:Width="16"
									Opacity="0"
									windows:FlowDirection="LeftToRight" />
								<Rectangle
									Name="IndeterminateGlyph"
									windows:Width="9"
									windows:Height="9"
									Fill="{windows:DynamicResource CheckBoxForegroundThemeBrush}"
									Opacity="0"
									windows:UseLayoutRounding="False" />
								<Rectangle
									Name="FocusVisualWhite"
									windows:UseLayoutRounding="True"
									IsHitTestVisible="False"
									Stroke="{windows:DynamicResource FocusVisualWhiteStrokeThemeBrush}"
									StrokeEndLineCap="Square"
									StrokeDashArray="1,1"
									Opacity="0"
									StrokeDashOffset="0.5"
									windows:Width="27"
									windows:Height="27" />
								<Rectangle
									Name="FocusVisualBlack"
									windows:UseLayoutRounding="True"
									IsHitTestVisible="False"
									Stroke="{windows:DynamicResource FocusVisualBlackStrokeThemeBrush}"
									StrokeEndLineCap="Square"
									StrokeDashArray="1,1"
									Opacity="0"
									StrokeDashOffset="1.5"
									windows:Width="27"
									windows:Height="27" />
							</Grid>
							<ContentPresenter
								Name="ContentPresenter"
								windows:Margin="{windows:TemplateBinding Padding}"
								windows:HorizontalAlignment="{windows:TemplateBinding HorizontalContentAlignment}"
								windows:VerticalAlignment="{windows:TemplateBinding VerticalContentAlignment}"
								Grid.Column="1" />
						</Grid>
					</Border>
				</ControlTemplate>
			</windows:Setter.Value>
		</windows:Setter>
	</windows:Style>
