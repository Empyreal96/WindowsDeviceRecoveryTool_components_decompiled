				<windows:Setter
					TargetName="ContentPresenter"
					Value="{DynamicResource ApplicationPointerOverForegroundThemeBrush}"
					Property="Foreground" />
			</windows:Trigger>
			<windows:Trigger
				Property="UIElement.IsMouseOver"
				Value="False">
				<windows:Setter
					TargetName="Arrow"
					Value="{DynamicResource ApplicationSecondaryForegroundThemeBrush}"
					Property="Fill" />
				<windows:Setter
					TargetName="ContentPresenter"
					Value="{DynamicResource ApplicationSecondaryForegroundThemeBrush}"
					Property="Foreground" />
			</windows:Trigger>
			<windows:Trigger
				Property="ToggleButton.IsChecked"
				Value="True">
				<windows:Setter
					TargetName="Arrow"
					Property="Visibility"
					Value="Hidden" />
				<windows:Setter
					TargetName="ContentPresenter"
					Value="{DynamicResource ApplicationPressedForegroundThemeBrush}"
					Property="Foreground" />
			</windows:Trigger>
			<windows:Trigger
				Property="IsEnabled"
				Value="False">
				<windows:Setter
					Value="{DynamicResource ComboBoxDisabledForegroundThemeBrush}"
					Property="Foreground" />
				<windows:Setter
					TargetName="Arrow"
					Value="{DynamicResource ComboBoxDisabledForegroundThemeBrush}"
					Property="Fill" />
				<windows:Setter
					TargetName="ContentPresenter"
					Value="{DynamicResource ComboBoxDisabledForegroundThemeBrush}"
					Property="Foreground" />
			</windows:Trigger>
		</ControlTemplate.Triggers>
	</ControlTemplate>
