	<windows:Style
		TargetType="{x:Type ComboBoxItem}"
		x:Key="{x:Type ComboBoxItem}">
		<windows:Setter
			Property="FrameworkElement.OverridesDefaultStyle"
			Value="True" />
		<windows:Setter
			Property="UIElement.SnapsToDevicePixels"
			Value="True" />
		<windows:Setter
			Property="FrameworkElement.FocusVisualStyle"
			Value="{x:Null}" />
		<windows:Setter
			Property="Background"
			Value="#00FFFFFF" />
		<windows:Setter
			Property="KeyboardNavigation.TabNavigation"
			Value="Local" />
		<windows:Setter
			Property="Padding"
			Value="8,10" />
		<windows:Setter
			Property="HorizontalContentAlignment"
			Value="Left" />
		<windows:Setter
			Property="FontSize"
			Value="16" />
		<windows:Setter
			Property="FontFamily"
			Value="{DynamicResource ContentControlThemeFontFamily}" />
		<windows:Setter
			Property="Typography.StylisticSet1"
			Value="True" />
		<windows:Setter
			Property="Typography.CaseSensitiveForms"
			Value="True" />
		<windows:Setter
			Property="Template">
			<windows:Setter.Value>
				<ControlTemplate
					TargetType="{x:Type ComboBoxItem}">
					<Border
						Name="LayoutRoot"
						Background="{windows:TemplateBinding Background}"
						BorderBrush="{windows:TemplateBinding BorderBrush}"
						BorderThickness="{windows:TemplateBinding BorderThickness}">
						<Grid
							Name="InnerGrid"
							Background="#00FFFFFF">
							<ContentPresenter
								Name="ContentPresenter"
								windows:HorizontalAlignment="{windows:TemplateBinding HorizontalContentAlignment}"
								windows:VerticalAlignment="{windows:TemplateBinding VerticalContentAlignment}"
								windows:Margin="{windows:TemplateBinding Padding}" />
							<Rectangle
								Name="FocusVisualWhite"
								windows:UseLayoutRounding="True"
								Stroke="{windows:DynamicResource FocusVisualWhiteStrokeThemeBrush}"
								StrokeEndLineCap="Square"
								StrokeDashArray="1,1"
								Opacity="0"
								StrokeDashOffset=".5" />
							<Rectangle
								Name="FocusVisualBlack"
								windows:UseLayoutRounding="True"
								Stroke="{windows:DynamicResource FocusVisualBlackStrokeThemeBrush}"
								StrokeEndLineCap="Square"
								StrokeDashArray="1,1"
								Opacity="0"
								StrokeDashOffset="1.5" />
						</Grid>
					</Border>
					<ControlTemplate.Triggers>
						<windows:Trigger
							Property="UIElement.IsMouseOver"
							Value="True">
							<windows:Setter
								TargetName="LayoutRoot"
								Value="{DynamicResource ComboBoxItemPointerOverBackgroundThemeBrush}"
								Property="Background" />
							<windows:Setter
								TargetName="ContentPresenter"
								Value="{DynamicResource ComboBoxItemPointerOverForegroundThemeBrush}"
								Property="Foreground" />
						</windows:Trigger>
						<windows:Trigger
							Property="ListBoxItem.IsSelected"
							Value="True">
							<windows:Setter
								TargetName="InnerGrid"
								Value="{DynamicResource ComboBoxItemSelectedBackgroundThemeBrush}"
								Property="Background" />
							<windows:Setter
								TargetName="ContentPresenter"
								Value="{DynamicResource ComboBoxItemSelectedForegroundThemeBrush}"
								Property="Foreground" />
						</windows:Trigger>
						<windows:MultiTrigger>
							<windows:MultiTrigger.Conditions>
								<windows:Condition
									Property="UIElement.IsMouseOver"
									Value="True" />
								<windows:Condition
									Property="ListBoxItem.IsSelected"
									Value="True" />
							</windows:MultiTrigger.Conditions>
							<windows:Setter
								TargetName="InnerGrid"
								Value="{DynamicResource ComboBoxItemSelectedPointerOverBackgroundThemeBrush}"
								Property="Background" />
						</windows:MultiTrigger>
						<windows:Trigger
							Property="IsEnabled"
							Value="False">
							<windows:Setter
								TargetName="ContentPresenter"
								Value="{DynamicResource ComboBoxItemDisabledForegroundThemeBrush}"
								Property="Foreground" />
