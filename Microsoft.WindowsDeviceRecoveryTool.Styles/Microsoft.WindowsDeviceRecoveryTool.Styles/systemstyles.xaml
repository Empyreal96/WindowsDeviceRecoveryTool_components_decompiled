						<Border
							Name="Bd"
							BorderBrush="{windows:TemplateBinding BorderBrush}"
							BorderThickness="{windows:TemplateBinding BorderThickness}"
							Background="{windows:TemplateBinding Background}"
							Padding="{windows:TemplateBinding Padding}"
