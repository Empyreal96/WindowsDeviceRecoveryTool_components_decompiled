							<windows:Setter
								TargetName="DropDownGlyph"
								Value="{DynamicResource ComboBoxArrowPressedForegroundThemeBrush}"
								Property="Foreground" />
							<windows:Setter
								TargetName="Highlight"
								Value="{DynamicResource ComboBoxPressedHighlightThemeBrush}"
								Property="Fill" />
						</windows:Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</windows:Setter.Value>
		</windows:Setter>
	</windows:Style>
	<ControlTemplate
